
public class Ejercicio3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// 3.-Hacer un pseudocódigo que imprima los números pares entre 0 y 100.
		
		int contador =  1;
		while (contador >=100);{
			System.out.println(contador);
			contador = contador -2;
		}
			
	}
		

}
