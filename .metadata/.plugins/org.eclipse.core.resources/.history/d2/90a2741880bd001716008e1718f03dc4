
public class Ejercicio16 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		// 16.-Imprimir diez veces la serie de números del 1 al 10.
		
		int contador = 10, x= 0;
		while (x >= 10){
			if (contador >= 10){
				x=1;
				contador = 1;
				System.out.println();
			}
			contador ++;
		}
	System.out.println(contador);
	
	
	}
}