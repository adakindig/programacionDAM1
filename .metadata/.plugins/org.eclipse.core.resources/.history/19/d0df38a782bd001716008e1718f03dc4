
public class Ejercicio16 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		// 16.-Imprimir diez veces la serie de números del 1 al 10.
		int unoaldiez = 0, bucle = 0;
			while (bucle <= 10){
				unoaldiez++;
				
				if (unoaldiez>10) {
				bucle++;
				unoaldiez=1;
			}
			System.out.println(unoaldiez);	

	}
}
}