
public class Ejercicio16 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		// 16.-Imprimir diez veces la serie de números del 1 al 10.
		int unoaldiez = 1, bucle = 0;
			while (unoaldiez <= 10){
				unoaldiez++;
				
				if (unoaldiez = 10) {
				bucle++;
			}
			
			System.out.println(unoaldiez);	
			
				
	
	}
}
}