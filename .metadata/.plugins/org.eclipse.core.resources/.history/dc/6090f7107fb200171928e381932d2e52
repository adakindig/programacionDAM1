import java.util.Scanner;
public class Ejercicio4 {

	public static void main(String[] args) {
		int sumap = 0;
		int sumai = 0;
		for(int i = 1; i <= 100; i++){
			System.out.println(i);
			if(i%2==0){
				sumap++;
			}
			else{
				sumai++;
			}
		}
		System.out.println("Hay "+sumap+" múltiplos de 2");
		System.out.println("Hay"+sumai+" múltiplos de 3");
		
	}
}
